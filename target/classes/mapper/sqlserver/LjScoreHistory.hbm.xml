<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.youthen.lj.persistence.entity">
	<class name="LjScoreHistory" table="LJ_SCORE_HISTORY">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="identity" />
		</id>
		<property name="userId" column="USER_ID" type="java.lang.String" />
		<property name="gotFrom" column="GOT_FROM" type="java.lang.String" />
		<property name="score" column="SCORE" type="java.lang.Long" />
		<property name="reason" column="REASON" type="java.lang.String" />
		<property name="createrId" column="CREATER_ID" type="java.lang.String" />
		<property name="createTime" column="CREATE_TIME" type="java.util.Date" />

	</class>

</hibernate-mapping>
